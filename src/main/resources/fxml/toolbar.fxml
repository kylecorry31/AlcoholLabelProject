<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="96.0" prefWidth="1024.0" style="-fx-background-color: -color-primary;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.emeraldElves.alcohollabelproject.UserInterface.ToolbarController">
   <children>
      <Label id="toolbar-title" layoutX="31.0" layoutY="-1.0" onMouseClicked="#goHome" prefHeight="96.0" prefWidth="323.0" text="EMERALD ELVES COLA" />
      <HBox alignment="CENTER_RIGHT" layoutX="395.0" layoutY="26.0" prefHeight="42.0" prefWidth="615.0" spacing="32.0">
         <children>
            <Button fx:id="extraButton" mnemonicParsing="false" onMouseClicked="#extraFunction" style="-fx-background-color: -swatch-green; -fx-font-size: 18;" text="PROFILE" wrapText="true">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Button>
            <Button fx:id="utility" mnemonicParsing="false" onMouseClicked="#utilityButton" style="-fx-background-color: -swatch-green; -fx-font-size: 18;" text="MY APPLICATIONS" wrapText="true">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Button>
            <Button fx:id="logButton" mnemonicParsing="false" onMouseClicked="#loadLog" style="-fx-background-color: -swatch-green; -fx-font-size: 18;" text="LOGIN" />
         </children>
      </HBox>
   </children>
   <styleClass>
      <String fx:value="tool-bar" />
      <String fx:value="toolbar-colored" />
   </styleClass>
</Pane>
